#ifndef IOMANAGER_H_
#define IOMANAGER_H_

class IOManager : public Informer {
private:
	void ButtonInt();
	void SwitchInt();
public:
	IOManager();
	virtual ~IOManager();
	char getButtons();
	char getSwitches();
	void setLeds(uint8_t, uint8_t);

};

#endif /* IOMANAGER_H_ */
